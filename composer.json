{
	"name": "mlantz/gondolyn2",
	"description": "A Seed Application built on Laravel 5.",
	"keywords": ["Gondolyn"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
		"patchwork/utf8": "1.1.*",
		"laravel/socialite": "~2.0",
		"devfactory/minify": "1.0.*",
		"anhskohbo/no-captcha": "*",
		"nesbot/Carbon": "*",
		"laravel/cashier": "*",
		"illuminate/html": "~5.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database",
			"app/Http/Controllers",
			"app/Console/Commands",
			"app/Models"
		],
	    "psr-0": {
	        "Tools": "app/Libraries",
	        "Validation": "app/Libraries",
	        "Gondolyn": "app/Libraries",
	        "Crypto": "app/Libraries"
	    },
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
